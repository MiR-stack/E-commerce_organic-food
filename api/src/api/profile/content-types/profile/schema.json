{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "firstName": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 12
    },
    "lastName": {
      "type": "string",
      "required": true,
      "maxLength": 12,
      "minLength": 3
    },
    "billing": {
      "type": "component",
      "repeatable": false,
      "component": "customer.billing",
      "required": false
    },
    "shipping": {
      "type": "component",
      "repeatable": true,
      "component": "customer.billing"
    },
    "customer_id": {
      "type": "string",
      "required": true
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "profile"
    },
    "users_permissions_customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "profile"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "profile"
    },
    "refunds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::refund.refund",
      "mappedBy": "profile"
    },
    "store_review": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store-review.store-review",
      "inversedBy": "profile"
    },
    "blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "profile"
    }
  }
}
